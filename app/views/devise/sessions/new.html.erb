<%= stylesheet_link_tag 'login' %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: 'btn btn-outline-primary btn-lg github-login-btn' %><br />
  <% end %>
<% end %>
